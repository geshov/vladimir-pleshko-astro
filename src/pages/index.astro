---
import Base from "../layouts/Base.astro";
import Hero from "../components/Hero.astro";
import History from "../components/History.astro";
import Competencies from "../components/Competencies.astro";

import { getEntry } from "astro:content";
const index = await getEntry("pages", "index");
---

<Base title={index.data.title} description={index.data.description}>
  <Hero />
  <History />
  <Competencies />
</Base>
