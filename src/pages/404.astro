---
import Base from "../layouts/Base.astro";
import Page404 from "../components/Page404.astro";

import { getEntry } from "astro:content";
const page = await getEntry("pages", "404");
---

<Base title={page.data.title} description={page.data.description}>
  <Page404 title={page.data.title} description={page.data.description} />
</Base>
